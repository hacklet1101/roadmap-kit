# ROADMAP-KIT Authentication Configuration
# Copia este archivo a .env y modifica los valores

# Habilitar autenticacion (true/false)
# Si es false, el dashboard es accesible sin login
ROADMAP_AUTH_ENABLED=true

# Usuario y contraseña para acceder al dashboard
ROADMAP_USER=admin
ROADMAP_PASSWORD=tu_contraseña_segura

# Puerto del servidor (opcional, default: 3001)
PORT=3001
